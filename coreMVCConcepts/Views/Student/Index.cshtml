
@{
    ViewData["Title"] = "Index";
}

<h1>@ViewBag.Message</h1>
<br/>
<p>There are @ViewBag.StudentCount participants have joined.</p>

<table class="table">
    <thead>
        <tr>
            <th>Student Id</th>
            <th>Name</th>
            <th>City</th>
            <th>Gender</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in ViewBag.StudentList)
            {
                <tr>
                    <td>@item.StudentId</td>
                    <td>@item.Name</td>
                    <td>@item.City</td>
                    <td>@item.Gender</td>
                </tr>
            }
    </tbody>
</table>

<br/>

<a asp-controller="Student" asp-action="Details">Go to Details</a>
