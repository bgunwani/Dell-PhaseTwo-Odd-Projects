
@model IEnumerable<coreMVCConcepts.Models.Student>

@{
    ViewData["Title"] = "StudentData";
}

<h1>StudentData</h1>

<table class="table">
        <thead>
            <tr>
                <th>Student Id</th>
                <th>Name</th>
                <th>City</th>
                <th>Gender</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model)
            {
            <tr>
                <td>@item.StudentId</td>
                <td>@item.Name</td>
                <td>@item.City</td>
                <td>@item.Gender</td>
            </tr>
            }
        </tbody>
</table>

